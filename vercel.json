{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "GOOGLE_API_KEY": "@google_api_key"
  }
}
